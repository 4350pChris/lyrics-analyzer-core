{
  "name": "@4350pchris/lyrics-analyzer-core",
  "version": "1.0.0",
  "description": "Analyze lyrics",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepublish": "tsc",
    "build": "tsc",
    "lint": "xo",
    "test": "xo && c8 ava",
    "test:watch": "xo && ava --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4350pChris/lyrics-analyzer-core.git"
  },
  "keywords": [
    "lyrics"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/4350pChris/lyrics-analyzer-core/issues"
  },
  "homepage": "https://github.com/4350pChris/lyrics-analyzer-core#readme",
  "dependencies": {
    "dynamoose": "^3.2.0"
  },
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "ava": "^5.2.0",
    "c8": "^7.13.0",
    "typescript": "^5.0.4",
    "xo": "^0.54.2"
  },
  "type": "module",
  "ava": {
    "files": [
      "test/**/*.ts"
    ],
    "typescript": {
      "rewritePaths": {
        "test/": "dist/test/"
      },
      "compile": false
    }
  },
  "xo": {
    "rules": {
      "radix": [
        "warn",
        "as-needed"
      ]
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack"
      ]
    }
  },
  "engines": {
    "node": "18"
  }
}

{
  "name": "@4350pchris/lyrics-analyzer-core",
  "version": "1.0.0",
  "description": "Analyze lyrics",
  "author": "",
  "main": "serverless.ts",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/4350pChris/lyrics-analyzer-core/issues"
  },
  "homepage": "https://github.com/4350pChris/lyrics-analyzer-core#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/4350pChris/lyrics-analyzer-core.git"
  },
  "keywords": [
    "lyrics"
  ],
  "type": "commonjs",
  "scripts": {
    "build": "rimraf dist && tsc",
    "lint": "xo",
    "test": "rimraf coverage && xo && c8 ava",
    "test:watch": "xo && ava --watch"
  },
  "dependencies": {
    "@aws-sdk/client-sqs": "^3.332.0",
    "@middy/core": "^4.4.0",
    "@middy/http-json-body-parser": "^4.4.0",
    "@middy/sqs-json-body-parser": "^2.5.7",
    "awilix": "^8.0.1",
    "dynamoose": "^3.2.0",
    "htmlparser2": "^8.0.2",
    "ofetch": "^1.0.1"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.30.1",
    "@types/aws-lambda": "^8.10.115",
    "@types/node": "^18.16.5",
    "ava": "^5.2.0",
    "c8": "^7.13.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.18",
    "json-schema-to-ts": "^2.8.0",
    "rimraf": "^5.0.0",
    "serverless-esbuild": "^1.44.0",
    "serverless-finch": "^4.0.3",
    "serverless-lift": "^1.26.0",
    "serverless-offline": "^12.0.4",
    "testdouble": "^3.18.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4",
    "xo": "^0.54.2"
  },
  "ava": {
    "files": [
      "test/**/*.ts"
    ],
    "extensions": [
      "ts"
    ],
    "environmentVariables": {
      "TS_NODE_LOG_ERROR": "true"
    },
    "require": [
      "ts-node/register"
    ]
  },
  "xo": {
    "rules": {
      "radix": [
        "warn",
        "as-needed"
      ],
      "import/extensions": "off",
      "unicorn/prefer-module": "off"
    }
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack"
      ]
    }
  },
  "engines": {
    "node": "18"
  }
}

{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "playwright test",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.13",
    "@vueuse/core": "^10.1.2",
    "ofetch": "^1.0.1",
    "vue": "^3.3.2",
    "vue-router": "^4.2.0",
    "vue3-toastify": "^0.1.11"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.1.10",
    "@playwright/test": "^1.33.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.8",
    "@typescript-eslint/parser": "^5.59.6",
    "@unocss/preset-attributify": "^0.51.13",
    "@unocss/preset-icons": "^0.51.13",
    "@unocss/preset-typography": "^0.51.13",
    "@unocss/preset-uno": "^0.51.13",
    "@unocss/preset-web-fonts": "^0.51.13",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^22.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "serverless-build-client": "^2.5.0",
    "serverless-finch": "^4.0.3",
    "typescript": "~5.0.4",
    "unocss": "^0.51.13",
    "unplugin-auto-import": "^0.16.0",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.3.5",
    "vitest": "^0.31.0",
    "vue-eslint-parser": "^9.3.0",
    "vue-tsc": "^1.6.4"
  }
}
